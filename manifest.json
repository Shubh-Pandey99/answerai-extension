{
  "manifest_version": 3,
  "name": "AI Meeting Assistant",
  "version": "1.0",
  "permissions": [
    "tabs",
    "activeTab",
    "scripting",
    "tabCapture",
    "offscreen"
  ],
  "host_permissions": [
    "https://answerai-extension-twq4.vercel.app/"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
